{
  "name": "relative-to-date",
  "version": "0.0.2",
  "description": "Converts relative time strings to date objects",
  "keywords": [
    "date",
    "timeago",
    "time ago",
    "relative time",
    "parser",
    "from now"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "lint": "eslint .",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Javier Alvarez <neiker@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neiker/relative-to-date.git"
  },
  "bugs": {
    "url": "https://github.com/neiker/relative-to-date/issues"
  },
  "homepage": "https://github.com/neiker/relative-to-date",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "3.10.1",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-plugin-import": "2.2.0",
    "mocha": "3.1.2",
    "pre-git": "3.10.0"
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "npm run lint"
      ],
      "pre-push": [
        "npm run test"
      ]
    }
  }
}
